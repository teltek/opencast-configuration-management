[opencast]
name = Opencast el 7 Repository
baseurl  = https://{{ opencast_repository_username }}:{{ opencast_repository_password }}@pkg.opencast.org/rpms/release/el/7/x86_64
enabled  = 1
gpgcheck = 1
gpgkey = https://pkg.opencast.org/gpgkeys/opencast-el7-rpm.opencast.org.key

[opencast-noarch]
name = Opencast el 7 Repository - noarch
baseurl  = https://{{ opencast_repository_username }}:{{ opencast_repository_password }}@pkg.opencast.org/rpms/release/el/7/noarch
enabled  = 1
gpgcheck = 1
gpgkey = https://pkg.opencast.org/gpgkeys/opencast-el7-rpm.opencast.org.key

[opencast-debuginfo]
name = Opencast el 7 Repository - debuginfo
baseurl  = https://{{ opencast_repository_username }}:{{ opencast_repository_password }}@pkg.opencast.org/rpms/release/el/7/debuginfo
enabled  = 0
gpgcheck = 1
gpgkey = https://pkg.opencast.org/gpgkeys/opencast-el7-rpm.opencast.org.key
